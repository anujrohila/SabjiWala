@model adminsite.Models.DTO.ordermasterdto
@{
    Layout = null;
}


@using (Html.BeginForm("OrderDetail", "Order"))
{
         
    <div id="page-content" >
        <div class="row" id="detail">
            <div class="col-sm-12">
                <div class="panel">
                    <div class="panel-heading">
                        <h3 class="panel-title">Order</h3>
                    </div>

                    <div class="panel-body">


                        <div class="form-group">
                            <label class="col-sm-3 control-label">Customer</label>
                            <div class="col-md-6 col-sm-6">
                                @Html.TextBoxFor(Model => Model.customer, new { @class = "form-control", @ViewBag.Disabled })
                            </div>
                        </div>
                        <br />

                     
                        <div class="form-group">
                            <label class="col-sm-3 control-label">Address</label>
                            <div class="col-md-6 col-sm-6">
                                @Html.TextBoxFor(Model => Model.address, new { @class = "form-control", @ViewBag.Disabled })
                            </div>
                        </div>
                        <br />



                        <div class="form-group">
                            <label class="col-sm-3 control-label">Order Status</label>
                            <div class="col-md-6 col-sm-6">
                                @Html.TextBoxFor(Model => Model.orderstatus, new { @class = "form-control", @ViewBag.Disabled })
                            </div>
                        </div>
                        <br />

                        <div class="form-group">
                            <label class="col-sm-3 control-label">Order Date-Time</label>
                            <div class="col-md-6 col-sm-6">
                                @Html.TextBoxFor(Model => Model.orderdatetime, new { @class = "form-control", @ViewBag.Disabled })
                            </div>
                        </div>
                        <br />

                        <div class="form-group">
                            <label class="col-sm-3 control-label">Employee</label>
                            <div class="col-md-6 col-sm-6">
                                @Html.TextBoxFor(Model => Model.employee, new { @class = "form-control", @ViewBag.Disabled })
                            </div>
                        </div>
                        <br />                                  
                       
                </div>
            </div>
        </div>
    </div>

       </div>
   
}
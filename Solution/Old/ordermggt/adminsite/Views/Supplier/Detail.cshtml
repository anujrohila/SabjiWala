@model adminsite.Models.DTO.supplierdto
@{
    ViewBag.Title = "Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}




<form class="form-horizontal">
  
    <div id="page-content">
        <div class="row">
            <div class="col-sm-12">
                <div class="panel">
                    <div class="panel-heading">
                        <h3 class="panel-title">Supplier information</h3>
                    </div>

                    <div class="panel-body">


                        <div class="form-group">
                            <label class="col-sm-3 control-label">Supplier Name(Mobile No.)</label>
                            <div class="col-md-6 col-sm-6">
                                @Html.TextBoxFor(Model => Model.suppname, new { @class = "form-control", @ViewBag.Disabled })
                            </div>
                        </div>


                       @* <div class="form-group">
                            <label class="col-sm-3 control-label">Password:</label>
                            <div class="col-md-6 col-sm-6">

                            </div>
                        </div>*@


                        @* <div class="row">
                             <div class="col-sm-12">
                            <div class="form-group">
                            <label class="control-label">Confirm Password:</label>
                            <i>@Html.PasswordFor(Model => Model.SupplierName, new { @class = "form-control" })</i>
                              </div>
                            </div>
                        </div>*@




                        <div class="form-group">
                            <label class="col-sm-3 control-label">First Name</label>
                            <div class="col-md-6 col-sm-6">
                                @Html.TextBoxFor(Model => Model.fname, new { @class = "form-control", @ViewBag.Disabled })
                            </div>
                        </div>


                        <div class="form-group">
                            <label class="col-sm-3 control-label">Middle Name</label>
                            <div class="col-md-6 col-sm-6">
                                @Html.TextBoxFor(Model => Model.mname, new { @class = "form-control", @ViewBag.Disabled })
                            </div>
                        </div>


                        <div class="form-group">
                            <label class="col-sm-3 control-label">Last Name</label>
                            <div class="col-md-6 col-sm-6">
                                @Html.TextBoxFor(Model => Model.lname, new { @class = "form-control", @ViewBag.Disabled })
                            </div>
                        </div>


                        <div class="form-group">
                            <label class="col-sm-3 control-label">EmailId</label>
                            <div class="col-md-6 col-sm-6">
                                <i>@Html.TextBoxFor(Model => Model.emailid, new { @class = "form-control", @ViewBag.Disabled })
                            </div>
                        </div>


                        <div class="form-group">
                            <label class="col-sm-3 control-label">Organization Name</label>
                            <div class="col-md-6 col-sm-6">
                                @Html.TextBoxFor(Model => Model.organization, new { @class = "form-control", @ViewBag.Disabled })
                            </div>
                        </div>



                        <div class="form-group">
                            <label class="col-sm-3 control-label">Language</label>
                            <div class="col-md-6 col-sm-6">
                                @Html.TextBoxFor(Model => Model.language, new { @class = "form-control", @ViewBag.Disabled })
                            </div>
                        </div>



                        <div class="form-group">
                            <label class="col-sm-3 control-label">Address</label>
                            <div class="col-md-6 col-sm-6">
                                @Html.TextBoxFor(Model => Model.address, new { @class = "form-control", @ViewBag.Disabled })
                            </div>
                        </div>





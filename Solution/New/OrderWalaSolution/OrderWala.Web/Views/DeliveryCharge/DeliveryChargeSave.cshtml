@model OrderWala.Domain.tblDeliveryChargeDTO
@{
    ViewBag.Title = "DeliveryChargeSave";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<h2>DeliveryChargeSave</h2>

<div id="page-title">
    <h1 class="page-header text-overflow">General Elements</h1>
</div>

<div class="panel">
    <div class="panel-heading">
        <h3 class="panel-title">State Master</h3>
    </div>
   
    
    <!--Input Size-->
    <!--===================================================-->
  @using (Html.BeginForm("DeliveryChargeSave", "DeliveryCharge", FormMethod.Post, new { @class = "form-horizontal" }))
    {
      
      @Html.HiddenFor(Model => Model.DeliveryChargesId)
      
        <div class="panel-body">
            @Html.ValidationSummary(true)
            <div class="form-group">
                <div class="text-danger">
                    @Html.ValidationSummary()
                </div>
            </div>
            
            <div class="form-group">
                <label class="col-sm-3 control-label" for="demo-is-inputsmall">Start Amount</label>
                <div class="col-sm-5">
                    @Html.TextBoxFor(model => model.StartAmount, new { @class ="form-control input-sm", placeholder ="" })
                    @*<input type="text" placeholder=".input-sm" class="form-control input-sm" id="demo-is-inputsmall">*@
                </div>
            </div>
            
            <div class="form-group">
                <label class="col-sm-3 control-label" for="demo-is-inputsmall">End Amount</label>
                <div class="col-sm-5">
                    @Html.TextBoxFor(model => model.EndAmount, new { @class ="form-control input-sm", placeholder ="" })
                    @*<input type="text" placeholder=".input-sm" class="form-control input-sm" id="demo-is-inputsmall">*@
                </div>
            </div>

            <div class="form-group">
                <label class="col-sm-3 control-label" for="demo-is-inputsmall">Charge</label>
                <div class="col-sm-5">
                    @Html.TextBoxFor(model => model.Charges, new { @class ="form-control input-sm", placeholder ="" })
                    @*<input type="text" placeholder=".input-sm" class="form-control input-sm" id="demo-is-inputsmall">*@
                </div>
            </div>
        </div>
        <div class="panel-footer">
            <div class="row">
                <div class="col-sm-9 col-sm-offset-3">
                    <button class="btn btn-success btn-labeled fa fa-save fa-lg" type="submit">Save</button>
                    <button class="btn btn-danger btn-labeled fa fa-repeat fa-lg" type="reset">Cancel</button>
                </div>
            </div>
        </div>
    }
    <!--===================================================-->
    <!--End Input Size-->


</div>

